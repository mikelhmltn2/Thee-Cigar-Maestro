{
  "name": "thee-cigar-maestro-api",
  "version": "1.0.0",
  "description": "Backend API for Thee Cigar Maestro - Comprehensive cigar enthusiast platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "docker:build": "docker build -t cigar-maestro-api .",
    "docker:run": "docker run -p 3000:3000 cigar-maestro-api",
    "docs": "apidoc -i routes/ -o docs/",
    "security:audit": "npm audit",
    "db:setup": "npm run migrate && npm run seed"
  },
  "keywords": [
    "cigar",
    "api",
    "express",
    "nodejs",
    "authentication",
    "recommendations",
    "analytics"
  ],
  "author": "Mike Hamilton",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^6.15.0",
    "express-session": "^1.17.3",
    "express-cors": "^2.8.5",
    "helmet": "^6.1.5",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "dotenv": "^16.0.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-facebook": "^3.0.0",
    "passport-jwt": "^4.0.1",
    "mongoose": "^7.0.3",
    "redis": "^4.6.5",
    "nodemailer": "^6.9.1",
    "joi": "^17.9.1",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.0",
    "uuid": "^9.0.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "crypto": "^1.0.1",
    "speakeasy": "^2.0.0",
    "qrcode": "^1.5.3",
    "socket.io": "^4.6.1",
    "ws": "^8.13.0",
    "node-cron": "^3.0.2",
    "winston": "^3.8.2",
    "express-winston": "^4.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2",
    "apidoc": "^0.29.0",
    "csvtojson": "^2.0.10",
    "fast-csv": "^4.3.6",
    "node-cache": "^5.1.2",
    "axios": "^1.3.6",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^19.11.1",
    "tensorflow": "^4.2.0",
    "@tensorflow/tfjs-node": "^4.2.0",
    "ml-matrix": "^6.10.4",
    "natural": "^6.1.0",
    "sentiment": "^5.0.2",
    "geoip-lite": "^1.4.7",
    "country-list": "^2.2.0",
    "express-slow-down": "^1.6.0",
    "express-brute": "^1.0.1",
    "express-brute-redis": "^0.0.1",
    "connect-redis": "^7.0.1",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "prettier": "^2.8.8",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "@types/jest": "^29.5.1",
    "cross-env": "^7.0.3",
    "concurrently": "^8.0.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mikelhmltn2/thee-cigar-maestro.git"
  },
  "bugs": {
    "url": "https://github.com/mikelhmltn2/thee-cigar-maestro/issues"
  },
  "homepage": "https://theecigarmaestro.vercel.app",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/docs/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "apidoc": {
    "name": "Thee Cigar Maestro API",
    "version": "1.0.0",
    "description": "Comprehensive API for cigar enthusiasts",
    "title": "Cigar Maestro API Documentation",
    "url": "https://api.theecigarmaestro.com"
  }
}