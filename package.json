{
  "name": "thee-cigar-maestro",
  "version": "2.0.0",
  "description": "Premium AI-driven cigar experience platform with luxury e-commerce and personalization",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit",
    "analyze": "cross-env ANALYZE=true next build",
    "automation:full": "node scripts/automated-code-analysis.js && node scripts/enhanced-performance-test.js",
    "automation:code": "node scripts/automated-code-analysis.js",
    "automation:performance": "node scripts/enhanced-performance-test.js",
    "automation:monitor": "node scripts/automated-monitoring.js",
    "automation:health": "node scripts/health-check.js",
    "automation:fix": "AUTO_FIX=true node scripts/automated-code-analysis.js",
    "automation:deploy": "npm run build && npm run automation:full",
    "automation:maintenance": "npm update && npm audit fix && npm run automation:health",
    "automation:dashboard": "node scripts/automation-dashboard.js",
    "autonomous:init": "node scripts/autonomous-init.js",
    "autonomous:start": "node scripts/autonomous-orchestrator.js",
    "autonomous:evolution": "node scripts/autonomous-evolution-engine.js",
    "autonomous:deploy": "node scripts/autonomous-deployment.js",
    "autonomous:status": "node scripts/autonomous-status.js",
    "autonomous:stop": "node scripts/autonomous-stop.js",
    "lighthouse": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "accessibility-test": "pa11y http://localhost:3000 --json > accessibility-report.json"
  },
  "keywords": [
    "cigars",
    "ai",
    "luxury",
    "e-commerce",
    "personalization",
    "next.js",
    "stripe",
    "three.js",
    "machine-learning"
  ],
  "author": "Mike Hamilton",
  "license": "MIT",
  "dependencies": {
    "next": "^14.2.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.5.4",
    "@next/bundle-analyzer": "^14.2.8",
    "three": "^0.160.0",
    "@types/three": "^0.160.0",
    "@react-three/fiber": "^8.15.12",
    "@react-three/drei": "^9.92.7",
    "framer-motion": "^10.16.16",
    "stripe": "^14.21.0",
    "@stripe/stripe-js": "^2.4.0",
    "next-auth": "^4.24.5",
    "prisma": "^5.7.1",
    "@prisma/client": "^5.7.1",
    "tailwindcss": "^3.4.1",
    "@tailwindcss/typography": "^0.5.10",
    "@tailwindcss/forms": "^0.5.7",
    "axios": "^1.6.5",
    "swr": "^2.2.4",
    "react-hook-form": "^7.48.2",
    "@hookform/resolvers": "^3.3.2",
    "zod": "^3.22.4",
    "openai": "^4.24.7",
    "lucide-react": "^0.303.0",
    "react-intersection-observer": "^9.5.3",
    "react-hot-toast": "^2.4.1",
    "sharp": "^0.33.2",
    "next-seo": "^6.4.0",
    "cookies-next": "^4.1.1",
    "react-confetti": "^6.1.0",
    "embla-carousel-react": "^8.0.0-rc19",
    "@headlessui/react": "^1.7.17",
    "react-lazyload": "^3.2.0",
    "js-yaml": "^4.1.0",
    "lighthouse": "^11.6.0",
    "pa11y": "^6.2.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.8",
    "postcss": "^8.4.33",
    "autoprefixer": "^10.4.16",
    "vitest": "^1.2.1",
    "@vitest/ui": "^1.2.1",
    "jsdom": "^24.0.0",
    "cross-env": "^7.0.3",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.2.0",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}